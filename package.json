{
  "name": "node-errors-helpers",
  "version": "0.1.2",
  "description": "Some helpers for better error handling in Node.js",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "lint": "eslint src test",
    "build": "babel --copy-files src/ -d build/",
    "cover": "istanbul cover _mocha --recursive",
