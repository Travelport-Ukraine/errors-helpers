  "version": "0.0.1",
  "description": "Some helpers for better error handling in Node.js",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src test examples",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
